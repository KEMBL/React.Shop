{
  "name": "localization",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint -c ../../.eslintrc.js --ext .ts,.tsx --ignore-pattern *.d.ts ./src",
    "lint:fix": "eslint -c ../../.eslintrc.js --ext .ts,.tsx --ignore-pattern *.d.ts --fix ./src",
    "format": "prettier --config ../../.prettierrc.js --ignore-path ../../.prettierignore --write src/**/*.{ts,json}"
  },
  "dependencies": {
    "he": "^1.2.0",
    "i18n-js": "^3.8.0",
    "lodash.memoize": "^4.1.2",
    "react-native-localize": "^1.4.3"
  },
  "devDependencies": {
    "@types/he": "^1.1.1",
    "@types/i18n-js": "^3.0.3",
    "@types/lodash.memoize": "^4.1.6"
  }
}
