{
  "name": "rns-mobile-app",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-gradle-clean": "cd ./android && ./gradlew clean && cd ../../..",
    "android-reset-metro-cache": "react-native start --reset-cache --verbose",
    "android-release": "npx react-native run-android --variant release --verbose",
    "info": "npx react-native info",
    "lint": "eslint -c ../../.eslintrc.js --ext .ts,.tsx --ignore-pattern *.d.ts ./index.tsx",
    "lint:fix": "eslint -c ../../.eslintrc.js --ext .ts,.tsx --ignore-pattern *.d.ts --fix ./index.tsx",
    "format": "yarn --version"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.1.5",
    "@react-navigation/stack": "^5.2.10",
    "react-native": "0.63.3",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-image-cache-hoc": "^2.0.0",
    "react-native-localize": "^1.4.3",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.11.0",
    "react-native-svg": "^12.1.0",
    "react-native-typescript-transformer": "1.2.13",
    "rn-fetch-blob": "^0.12.0",
    "rns-core": "0.0.3"
  },
  "devDependencies": {}
}
