{
  "name": "rns-packages",
  "version": "0.0.1",
  "private": true,
  "description": "Application redux packages grouped by entities for using with redux-package-loader",
  "scripts": {
    "lint": "eslint -c ../../.eslintrc.js --ext .ts,.tsx --ignore-pattern *.d.ts ./src",
    "lint:fix": "eslint -c ../../.eslintrc.js --ext .ts,.tsx --ignore-pattern *.d.ts --fix ./src",
    "format": "prettier --config ../../.prettierrc.js --ignore-path ../../.prettierignore --write \"{index.ts,src/**/*.{ts,tsx}}\""
  },
  "dependencies": {
    "@apollo/client": "^3.2.4",
    "@react-native-async-storage/async-storage": "^1.13.2",
    "debug": "^4.2.0",
    "immer": "^9.0.6",
    "lodash.merge": "^4.6.2",
    "redux-logger": "^3.0.6",
    "redux-package-loader": "^1.0.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-saga-creator": "^2.0.1",
    "reselect": "^4.0.0",
    "rns-types": "0.0.1",
    "robodux": "^10.0.0",
    "uuid": "~8.3.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/lodash.merge": "^4.6.6",
    "@types/uuid": "^8.3.0"
  }
}
