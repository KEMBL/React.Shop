{
  "name": "rns-packages",
  "version": "0.0.1",
  "private": true,
  "description": "Application redux packages grouped by entities for using with redux-package-loader",
  "scripts": {
    "lint": "eslint -c ../../.eslintrc.js --ext .ts,.tsx --ignore-pattern *.d.ts ./src",
    "lint:fix": "eslint -c ../../.eslintrc.js --ext .ts,.tsx --ignore-pattern *.d.ts --fix ./src",
    "format": "prettier --config ../../.prettierrc.js --ignore-path ../../.prettierignore --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@apollo/client": "^3.2.2",
    "debug": "^4.2.0",
    "immer": "^7.0.9",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "redux-batched-actions": "^0.5.0",
    "redux-logger": "^3.0.6",
    "redux-package-loader": "^1.0.2",
    "redux-saga": "^1.1.3",
    "redux-saga-creator": "^2.0.1",
    "reselect": "^4.0.0",
    "robodux": "^9.1.0",
    "rns-types": "0.0.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.merge": "^4.6.6"
  }
}
