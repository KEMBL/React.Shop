{
  "name": "rns-web-app",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "compile": "tsc -b --incremental --verbose --lib dom",
    "web": ".\\node_modules\\.bin\\webpack-cli --config webpack.config.js --watch",
