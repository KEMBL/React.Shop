{
  "compilerOptions": {
    "noEmit": true,
    "incremental": false,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es6"],
    "moduleResolution": "node",
    "strict": true,
    "target": "esnext",
    "resolveJsonModule": true,
    "noEmitOnError": true
  }
}
